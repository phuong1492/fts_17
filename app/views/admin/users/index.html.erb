<% provide(:title, 'All users') %>
<div class="row">
  <div class="box">
    <div class="col-lg-12">
      <hr>
      <h2 class="intro-text text-center"><strong>All users</strong></h2>
      <hr>
      <%= link_to "Add new Trainee", "#", class: "edit-btn btn btn-primary col-md-4 col-md-offset-4" %>
    </div>
    <ul>
      <div class="col-md-offset-9"><%= will_paginate %></div>
      <% @users.each do |user| %>
        <%= render user %>
      <% end %>
      <div class="col-md-offset-9"><%= will_paginate %></div>
    </ul>
  </div>
</div>
