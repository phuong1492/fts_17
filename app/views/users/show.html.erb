<% provide(:title, @user.username) %>
<div class="row">
  <div class="box">
    <div class="col-lg-12">
      <hr>
      <h2 class="intro-text text-center"><strong>Information</strong></h2>
      <hr>
      <aside class = "col-md-6 col-md-offset-4">
        <p>Username:<%= @user.username %></p>
        <p>Email: <%= @user.email %></p>
        <p>Created at: <%= @user.created_at %></p>
      </aside>
    </div>
  </div>
</div>